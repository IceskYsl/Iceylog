<% content_for :sitemap do %>
  <span class="current"><%= t("cpanel.menu.photos") %></span>
<% end %>
<h1>Listing photos</h1>

<table class="grid">
  <tr class="head">
    <td class="first">Id</td>
		<td>image</td>
		<td>time</td>
    <td></td>
  </tr>

<% @photos.each do |photo| %>
  <tr>
    
    <td class="first"><%= photo.id %></td>
		<td>
			<%= photo.filename %>(	<%= photo.content_type %>)	<br />		
			<%= image_tag(photo.image.thumb.url ) %>
		</td>
		<td><%= photo.created_at %></td>
    <td> 
    <%= link_to '', cpanel_photo_path(photo), :confirm => 'Are you sure?', :method => :delete, :class => "icon small_delete" %></td>
  </tr>
<% end %>
</table>

<br />

 

